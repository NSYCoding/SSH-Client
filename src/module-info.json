{
    "name": "SSH-Client",
    "id": "NSY.ssh_client",
    "version": "1.0.0",
    "author": "noursy",
    "author-id": "684fd894b2e21f06cdd3692b",
    "description": "A simple SSH client built for Nexus",
    "link": "https://github.com/NSYCoding/SSH-Client",
    "platforms": [
        "win32",
        "linux",
        "darwin"
    ],
    "build": {
        "build-version": 5,
        "excluded": [
            "electron.ts",
            "./renderer/react-wrapper"
        ],
        "included": [
            "./renderer/react-wrapper/react_module"
        ],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts",
                    "./renderer/renderer.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}